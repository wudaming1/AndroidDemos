<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aries.android.demo">

    <application
        android:name="com.aries.android.demo.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.aries.android.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aries.android.demo.nestedscrolling.NestedScrollingActivity1" />
        <activity android:name="com.aries.android.demo.touch.TouchActivity" />
        <activity android:name="com.aries.android.demo.nestedscrolling.NestedScrollingActivity2" />
        <activity android:name="com.aries.android.demo.headrecyclerview.HeadRecyclerViewActivity" />
        <activity android:name="com.aries.android.demo.headrecyclerview.ZoomHeadActivity"></activity>
    </application>

</manifest>